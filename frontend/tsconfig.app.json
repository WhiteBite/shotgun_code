{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/types/**/*.ts", "wailsjs/**/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "#wailsjs/*": ["./wailsjs/*"]
    },
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node"
  }
}